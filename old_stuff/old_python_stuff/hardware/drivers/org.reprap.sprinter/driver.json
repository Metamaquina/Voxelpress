{"backend" : "serial",
 "detect cmd": ["python", "detect.py"],
 "print cmd" : ["python", "runjob.py"],
 "warmup cmd": ["python", "warmup.py"],
 "manual mode": 0,
 "job format" : "gcode",
 "constants" : {
     "gcode" : {
	 "get temp" : "M105",
	 "warmup" : "M104 S{0}",
	 "use mm" : "G21",
	 "relative" : "G91",
	 "absolute" : "G90",
	 "home" : "G28",
	 "x home" : "G28 X0",
	 "y home" : "G28 Y0",
	 "z home" : "G28 Z0",
	 "rest" : "M84",
	 "move" : "G0 X{0} Y{1}",
	 "z move" : "G0 Z{0}",
	 "extrude" : "G1 E{0} F{1}"}}}